<% for (var i =0; i<cadastro.length;i++){%>
  <tr>
    <td><%=cadastro[i].Nome%></td>
    <td><%=cadastro[i].Registro%></td>
    <td><%=cadastro[i].Telefone%></td>
    <td><%=cadastro[i].Celular%></td>
    <td><%=cadastro[i].CEP%></td>
    <td><%=cadastro[i].Endereco%> <%=cadastro[i].Numero%> <%if (cadastro[i].Complemento!=''){%>,<%}%> <%=cadastro[i].Complemento%> - <%=cadastro[i].Bairro%></td>
    <td><%=cadastro[i].Cidade%> - <%=cadastro[i].UF%></td>
    <td><%for(var j = 0; j<cadastro[i].Especialidades.length;j++){%><%=cadastro[i].Especialidades[j]%><%if (j+1!=cadastro[i].Especialidades.length){%>, <%}}%></td>
    <td>
      <a
        href="/update-user?id=<%=cadastro[i]._id%>"
        class="btn border-shadow update"
      >
        <span class="text-greadient"><i class="fas fa-pencil-alt"></i></span>
      </a>
      <a class="btn border-shadow delete" data-id=<%=cadastro[i]._id%>>
        <span class="text-greadient"><i class="fas fa-times"></i></span>
      </a>
    </td>
  </tr>
  <%}%></cadastro.length;i++){%
>
