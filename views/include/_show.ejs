    <td><%=cadastro[i].Nome%></td>
    <td><%=cadastro[i].CRM%></td>
    <td><%=cadastro[i].Telefone%></td>
    <td><%=cadastro[i].Celular%></td>
    <td><%=cadastro[i].CEP%></td>
    <td>
      <a
        href="/update-user?id=<%=cadastro[i]._id%>"
