<!--include header-->
<%-include('include/_header')%>
<!--/include header-->

<!--Main Site-->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient"
          >Novo Cadastro<i class="fas fa-user"></i
        ></span>
      </a>
      <form class="search-engine" method="GET" action="/" id="search_user">
        <div class="form-group">
          <label for="search" class="text-light"></label>
          <input type="text" name="Search" placeholder ="Digite aqui" value="" />
        </div>
        <div class="form-group submit-button">
          <button type="submit" class="btn text-dark update">Pesquisar</button>
        </div>
      </form>
    </div>
    <!--form handling-->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>Nome</th>
            <th>Registro</th>
            <th>Telefone Fixo</th>
            <th>Telefone Celular</th>
            <th>CEP</th>
            <th>Endereco</th>
            <th>Cidade</th>
            <th>Especialidades</th>
            <th>Ações</th>
          </tr>
        </thead>
        <tbody>
          <%-include('include/_show')%>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!--/Main Site-->

<!--include footer-->
<%-include('include/_footer')%>
<!--/include footer-->
